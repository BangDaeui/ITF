                        <li class="nav-item active">
                            <a class="nav-link" href="/Setting">
                                <i class="material-icons">settings</i>
                                <span>Setting</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </aside>
            <!-- End Main Sidebar -->
            <main class="main-content col-lg-10 col-md-9 col-sm-12 p-0 offset-lg-2 offset-md-3">
                <div class="main-navbar sticky-top bg-white">
                    <!-- Main Navbar -->
                    <nav class="navbar align-items-stretch navbar-light flex-md-nowrap p-2">
                        <form action="#" class="main-navbar__search w-100 d-none d-md-flex d-lg-flex">

                        </form>
                        <ul class="navbar-nav border-left flex-row ">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle text-nowrap px-3" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">

                                    <span class="d-none d-md-inline-block">Administrator</span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-small">
                                    <a class="dropdown-item" href="/Setting">
                                        <i class="material-icons">settings</i> Setting</a>
                                    <!--
                                    <a class="dropdown-item" href="#">
                                        <i class="material-icons">vertical_split</i> Blog Posts</a>
                                    <a class="dropdown-item" href="#">
                                        <i class="material-icons">note_add</i> Add New Post</a>
                                    -->
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item text-danger" href="/Logout">
                                        <i class="material-icons text-danger">&#xE879;</i> Logout </a>
                                </div>
                            </li>
                        </ul>

                    </nav>
                </div>
                <!-- / .main-navbar -->
                <div class="main-content-container container-fluid px-4">
                    <!-- Page Header -->
                    <div class="page-header row no-gutters py-4 mb-3 border-bottom">
                        <div class="col-12 col-sm-4 text-center text-sm-left mb-0">
                            <span class="text-uppercase page-subtitle">in the forest</span>
                            <h3 class="page-title">Setting</h3>
                        </div>
                    </div>
                    <!-- End Page Header -->
                    <!-- Passsword 입력 -->
                    <div class="row mb-0">
                        <div class="col-10">
                            <div class="row">
                                <div class="form-group col-md-7">
                                    <div class="input-group">
                                        <input type="password" class="form-control" name="Pass" placeholder="Password">
                                        <span class="input-group-append">
                                            <span class="input-group-text">
                                                <i class="fa fa-lock"></i>
                                            </span>
                                        </span>
                                    </div>
                                </div>
                                <div class="form-group col-md-7">
                                <button type="submit" class="btn btn-dark btn-block" data-toggle="modal" data-target="#Usersettingmodal">Login</button>
                              </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Password -->
                    <!--Login Model-->
                    <form method=post action="/UserSetting">
                        <div class="modal fade" id="Usersettingmodal" tabindex="-1" role="dialog" aria-labelledby="UsersettingmodalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-lg" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="UsersettingmodalLabel">Setting</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="col">
                                                <h2>관리자 설정</h2>
                                                <div class="form-row">
                                                  <div class="form-group col-md-6">

                                                  </div>
                                                </div>
                                                <div class="form-row">
                                                  <div class="form-group col-md-6">

                                                  </div>
                                                </div>
                                                <div class="form-row">
                                                  <div class="form-group col-md-6">

                                                  </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="form-group col-md-6">
                                                      <button type="submit" class="btn btn-dark " data-dismiss="modal" data-toggle="modal" data-target="#CPSettingmodal">비밀번호 변경</button>
                                                      <button type="submit" class="btn btn-dark " data-dismiss="modal" data-toggle="modal" data-target="#CDSettingmodal">부서 추가/제거</button>
                                                      <button type="submit" class="btn btn-dark " data-dismiss="modal" data-toggle="modal" data-target="#CPOsSettingmodal">직급 추가/제거</button>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--End Login Model-->
                        <!--Change Password Model-->
                            <div class="modal fade" id="CPSettingmodal" tabindex="-1" role="dialog" aria-labelledby="CPSettingmodalLabel" aria-hidden="true">
                                <div class="modal-dialog modal-lg" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title" id="CPSettingmodalLabel">Setting</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="row">
                                                <div class="col">
                                                    <h2>비밀번호 변경</h2>
                                                    <div class="form-row">
                                                      <div class="form-group col-md-6">
                                                          <label for="WebAuth_Pass">기존 비밀번호</label>
                                                          <input name="WebAuth_Pass" id="WebAuth_Pass" class="form-control" >
                                                              {{#each WebAuth_Pass}}
                                                              <option  value="{{this.WebAuth_Pass}}" name="WebAuth_Pass">{{this.WebAuth_Pass}}</option>
                                                              {{/each}}
                                                          </input>
                                                      </div>
                                                    </div>
                                                    <div class="form-row">
                                                      <div class="form-group col-md-6">
                                                          <label for="WebAuth_Pass">변경할 비밀번호</label>
                                                          <input name="WebAuth_Pass" id="WebAuth_Pass" class="form-control" >
                                                              {{#each WebAuth_Pass}}
                                                              <option  value="{{this.WebAuth_Pass}}" name="WebAuth_Pass">{{this.WebAuth_Pass}}</option>
                                                              {{/each}}
                                                          </input>
                                                      </div>
                                                    </div>
                                                    <div class="form-row">
                                                        <div class="form-group col-md-6">
                                                          <button type="submit" class="btn btn-dark " data-toggle="" data-target="">변경</button>
                                                          <button type="button" class="btn btn-outline-primary" data-dismiss="modal">취소</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <!--End Change Password Model-->
                        <!--Change Department Model-->
                            <div class="modal fade" id="CDSettingmodal" tabindex="-1" role="dialog" aria-labelledby="CDSettingmodalLabel" aria-hidden="true">
